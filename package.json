{
  "name": "passman",
  "version": "1.0.0",
  "description": "Password Manager built with Electron",
  "main": "main.js",
  "scripts": {
    "start": "vite",
    "electron:dev": "concurrently \"npm:dev\" \"npm:electron\"",
    "dev": "vite",
    "electron": "electron ."
  },
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "vite": "^4.0.0",
    "electron": "^25.0.0",
    "electron-builder": "^24.6.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.32.0"
  },
  "dependencies": {
    "sqlite3": "^5.1.6",
    "crypto-js": "^4.1.1",
    "vue": "^3.2.0",
    "axios": "^1.5.0"
  },
  "build": {
    "appId": "com.cloudintel.passman",
    "productName": "PassMan",
    "asar": true,
    "compression": "maximum",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!**/*.map",
      "!node_modules/.cache"
    ],
    "win": {
      "target": "portable",
      "icon": "build/passman.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/passman.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build"
    }
  }
}
